{
    "editor.rulers": [
        79,
        120
    ],
    "files.associations": {
        "*.h": "cpp"
    },
    "C_Cpp.default.cppStandard": "c++11",
    "C_Cpp.default.cStandard": "c99",
    "C_Cpp.vcpkg.enabled": true,
    "editor.formatOnSave": true,
    "cmake.sourceDirectory": "${workspaceFolder}",
    "cmake.configureSettings": {
        "BUILD_SAMPLES": "ON",
        "BUILD_TESTS": "ON"
    },
    "cmake.debugConfig": {
        // Workaround for launching the debugger from the CMake toolbar.
        // Running from the CMake tools toolbar without a debugger will still have working directory issues.
        "cwd": "${workspaceFolder}"
    },
    "doxdocgen.c.triggerSequence": "/**",
